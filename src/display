# 01
n_1:
    li a0, 0x100 # set indivisual dot
    li a1, 0x00010001 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010002 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010003 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010004 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010005 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010006 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall
    li a1, 0x00010007 # a1[31:16] is x, [15:0] is y
    li a2, 0x00FF0000 #red
    ecall

    jal clear_display

